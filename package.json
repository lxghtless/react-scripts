{
  "name": "@lxghtless/react-scripts",
  "version": "0.2.1",
  "description": "scripts for developing, linting, building and testing react apps",
  "license": "ISC",
  "repository": "lxghtless/react-scripts",
  "author": {
    "name": "lxghtlxss",
    "email": "lxghtl3ss@gmail.com",
    "url": "github.com/lxghtless"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {},
  "bin": {
    "react-scripts": "./bin/reactScripts.js"
  },
  "files": [
    "bin",
    "scripts"
  ],
  "keywords": [
    "react-scripts",
    "react",
    "node"
  ],
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/plugin-transform-destructuring": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "address": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "chalk": "^2.4.2",
    "clean-stack": "^2.1.0",
    "css-loader": "^2.1.1",
    "eslint-config-xo": "^0.26.0",
    "eslint-config-xo-flow": "^0.1.0",
    "eslint-config-xo-react": "^0.19.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "execa": "^1.0.0",
    "file-loader": "^3.0.1",
    "fs-extra": "^8.0.1",
    "html-webpack-plugin": "^3.2.0",
    "meow": "^5.0.0",
    "open": "^6.3.0",
    "ora": "^3.4.0",
    "pretty-bytes": "^5.2.0",
    "pretty-ms": "^5.0.0",
    "prettyjson": "^1.2.1",
    "restore-cursor": "^3.1.0",
    "rimraf": "^2.6.3",
    "shell-quote": "^1.6.1",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.31.0",
    "webpack-dev-server": "^3.4.1",
    "webpack-manifest-plugin": "^2.0.4",
    "xo": "^0.24.0"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ],
      "import/extensions": [
        "js"
      ],
      "import/no-unassigned-import": 0,
      "capitalized-comments": 0
    },
    "settings": {
      "import/resolver": {
        "babel-module": {
          "root": [
            "./"
          ],
          "alias": {
            "scripts": "./"
          }
        }
      }
    },
    "parser": "babel-eslint"
  }
}
